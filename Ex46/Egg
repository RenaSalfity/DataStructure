import java.util.Date;
public class Egg {
	private int id;
	private double weight;
	private Date date;
	public Egg(int id, double weight, Date date) 
	{
	this.id=id;
	this.weight=weight;
	this.date=date;
	}
	public int getId() 
	{
	return id;
	}
	public double getWeight() 
	{
	return weight;
	}
	public Date getDate() {
		return date;
	}
	public String toString() {
		return "Egg{"+
				"id= "+id+
				", weight= "+ weight+
				", date= " + date+ " }";
					
	}
	
}
